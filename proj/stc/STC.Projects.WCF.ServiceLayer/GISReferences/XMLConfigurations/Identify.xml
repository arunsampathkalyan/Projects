<IdentifyConfiguration Tolerance="100" PointBuffer="1000" PolygonBuffer="500">
  <MapServices>
    <MapService ID="1">
      <Layers>
        <Layer ID="0">
          <Tabs>
            <Tab Type="BasicInfo" Name="Basic Info">
              <Fields>
                <Field ID="0" />
              </Fields>
            </Tab>
            <Tab Type="DetaildInfo" Name="Detailed Info">
              <Fields>
                <Field ID="2" />
              </Fields>
            </Tab>
            <Tab Type="LiveStream" Name="LiveStream" />
            <Tab Type="HistoricalStream" Name="HistoricalStream" />
          </Tabs>
          <Actions>
            <Action Type="Zoom" />
            <Action Type="Highlight" />
            <Action Type="Coverage" />
          </Actions>
        </Layer>
        <Layer ID="1">
          <Tabs>
            <Tab Type="BasicInfo" Name="Basic Info">
              <Fields>
                <Field ID="3" />
              </Fields>
            </Tab>
            <Tab Type="LiveStream" Name="LiveStream" />
            <Tab Type="HistoricalStream" Name="HistoricalStream" />
          </Tabs>
          <Actions>
            <Action Type="Zoom" />
            <Action Type="Highlight" />
            <Action Type="Route" />
            <Action Type="ServiceArea" >
              <TimeRanges>
                <Range Time="5"  Color="50, 255, 0,0"/>
                <Range Time="10"  Color="50, 0, 255,0"/>
                <Range Time="15"  Color="50, 0, 0,255"/>
              </TimeRanges>
            </Action>
          </Actions>
        </Layer>
        <Layer ID="2">
          <Tabs>
            <Tab Type="BasicInfo" Name="Basic Info">
              <Fields>
                <Field ID="4" />
              </Fields>
            </Tab>
            <Tab Type="NearestNetworkAssets" Name="NearestNetworkAssets" Radius="2000" LayerId="0" />
            <Tab Type="NearestMobileAssets" Name="NearestMobileAssets" Radius="2000" LayerId="1" >
              <ColorRanges>
                <Range From="0" To="1" Color="255, 255, 0,255"/>
                <Range From="1" To="4" Color="255, 255, 0,255"/>
                <Range From="2" To="5"  Color="255, 0, 255,255"/>
                <Range From="3" To="6"  Color="255, 0, 255,255"/>
                <Range From="4"   Color="255, 255, 0,0"/>
              </ColorRanges>
            </Tab>
          </Tabs>
          <Actions>
            <Action Type="Zoom" />
            <Action Type="Highlight" />
          </Actions>
        </Layer>

      </Layers>
    </MapService>
  </MapServices>
</IdentifyConfiguration>


